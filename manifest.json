{
    "manifest_version": 2,

    "name": "Baseball Hackday 2016",
    "description": "A cool chrome extension to visualize data in mlb gameday",
    "version": "1.0",

    "browser_action": {
        "default_icon": "icon.png"
    },

    "content_scripts": [
    {
        "matches":[
            "http://mlb.mlb.com/mlb/gameday/index.jsp*"
            ],
        "js": ["jquery-2.2.2.min.js", "content.js"]
    }
    ]
}
